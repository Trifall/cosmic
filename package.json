{
	"name": "cosmic",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "bunx --bun vite dev",
		"build": "NODE_ENV=production bunx --bun vite build",
		"start": "NODE_ENV=production bun run build/index.js",
		"preview": "bunx --bun vite preview",
		"prepare": "husky && svelte-kit sync || echo ''",
		"test": "bunx --bun vitest run",
		"check": "svelte-kit sync && svelte-check --workspace ./ --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --workspace ./ --tsconfig ./tsconfig.json --watch",
		"format": "prettier -c --ignore-path .gitignore --log-level warn \"./**/*.{js,jsx,ts,tsx,svelte,css,json}\" ",
		"format:fix": "prettier --ignore-path .gitignore --log-level warn --write \"./**/*.{js,jsx,ts,tsx,svelte,css,json}\" ",
		"lint": "oxlint ./src && eslint \"./src/**/*.svelte\" --cache",
		"lint:fix": "oxlint ./src --fix && eslint \"./src/**/*.svelte\" --cache --fix",
		"lint:oxlint": "oxlint ./src",
		"lint:eslint": "eslint \"./src/**/*.svelte\" --cache",
		"fullcheck": "bun run test && bun run check && bun run lint:fix && bun run format:fix && bun run build",
		"update-packages": "bun update",
		"db:generate": "bunx drizzle-kit generate --config ./database/drizzle.config.ts",
		"db:migrate": "bunx drizzle-kit migrate --config ./database/drizzle.config.ts",
		"db:push": "bunx drizzle-kit push --config ./database/drizzle.config.ts",
		"db:auth-generate": "bun x @better-auth/cli@latest generate --y --config ./src/lib/server/auth.ts --output ./database/schema/auth.schema.ts && bun run format:fix",
		"db:deploy": "bun run db:migrate"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/dompurify": "^3.2.0",
		"@types/eslint": "^9.6.1",
		"@types/fs-extra": "^11.0.4",
		"@types/mime-types": "^3.0.1",
		"bun-types": "^1.3.2",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-oxlint": "^1.28.0",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"oxlint": "^1.28.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.4",
		"vitest": "^4.0.9"
	},
	"dependencies": {
		"@lucide/svelte": "^0.554.0",
		"@sveltejs/enhanced-img": "^0.8.5",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/typography": "^0.5.19",
		"@tanstack/table-core": "^8.21.3",
		"autoprefixer": "^10.4.22",
		"better-auth": "^1.3.34",
		"bits-ui": "1.5.2",
		"clsx": "^2.1.1",
		"croner": "^9.1.0",
		"date-fns": "^4.1.0",
		"drizzle-kit": "^0.31.7",
		"drizzle-orm": "^0.44.7",
		"fs-extra": "^11.3.2",
		"highlight.js": "^11.11.1",
		"highlight.svelte": "^0.1.3",
		"isomorphic-dompurify": "^2.32.0",
		"marked": "^16.4.2",
		"mime-types": "^3.0.1",
		"mode-watcher": "^1.1.0",
		"postgres": "3.4.7",
		"rate-limiter-flexible": "^8.2.1",
		"runed": "^0.36.0",
		"svelte": "^5.43.8",
		"svelte-adapter-bun": "0.5.2",
		"svelte-sonner": "^1.0.6",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "0.2.1",
		"tailwindcss": "3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"tippy.js": "^6.3.7",
		"vaul-svelte": "^1.0.0-next.7",
		"vite": "7.2.2",
		"winston": "^3.18.3",
		"zip-a-folder": "^3.1.9",
		"zod": "^4.1.12"
	},
	"engines": {
		"node": "^22.0.0"
	},
	"packageManager": "bun@1.3.1"
}
